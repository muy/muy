{
  "name": "muy",
  "private": false,
  "version": "0.0.0",
  "description": "Muy main package",
  "main": "dist/index.js",
  "keywords": [
    "muy"
  ],
  "author": "Muy",
  "license": "ISC",
  "scripts": {
    "alex": "alex .",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "changelog": "lerna-changelog",
    "deploy": "lerna publish",
    "format": "prettier --write packages/*/{lib,lib/**}/*.{js,jsx}",
    "postinstall": "npm run bootstrap",
    "postcheckout": "npm run bootstrap",
    "precommit": "npm run alex && npm test",
    "prepare": "lerna run prepare",
    "prepush": "npm run alex && npm test",
    "pretest": "lerna run pretest",
    "preversion": "lerna run preversion",
    "prewatch": "npm run bootstrap",
    "test": "lerna run test",
    "version": "lerna run version",
    "watch": "lerna exec --parallel -- rollup -f cjs -i src/index.js -o dist/index.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muy/muy.git"
  },
  "bugs": {
    "url": "https://github.com/muy/muy/issues"
  },
  "homepage": "https://github.com/muy/muy#readme",
  "devDependencies": {
    "alex": "8.1.1",
    "husky": "4.2.5",
    "lerna": "3.20.2",
    "lerna-changelog": "1.0.1",
    "prettier": "2.0.4",
    "rollup": "2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush",
      "post-checkout": "npm run postcheckout"
    }
  },
  "changelog": {
    "labels": {
      "bug": "Bug Fix",
      "dependencies": "Dependency update",
      "documentation": "Docs",
      "enhancement": "New Feature"
    }
  }
}
